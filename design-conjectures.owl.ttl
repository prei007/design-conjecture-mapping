<http://www.coolfutures.net/rdf/Design_Conjectures/dcon#> rdf:type owl:Ontology ;
                                                           owl:imports <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#> ;
                                                           <http://purl.org/dc/terms/creator> "Peter Reimann"^^xsd:string ;
                                                           rdfs:comment "Initial work on an ontology for design conjecture claims."@en ;
                                                           owl:versionInfo "version 1.0"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#hasConjecture
:hasConjecture rdf:type owl:ObjectProperty ;
               rdfs:comment "Any design element can have a conjecture attached."^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#hasDesignFeature
:hasDesignFeature rdf:type owl:ObjectProperty ;
                  rdfs:domain :LearningDesign ;
                  rdfs:range :DesignFeature ;
                  rdfs:comment "A LearningDesign has a DesignFeature"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#respondsTo
:respondsTo rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:comment "For instance, a LearningDesign responds to an InstructionalChallenge"^^xsd:string .


#################################################################
#    Data properties
#################################################################

###  http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text
<http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> rdf:type owl:FunctionalProperty .


#################################################################
#    Classes
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#AttitudeOutcome
:AttitudeOutcome rdf:type owl:Class ;
                 rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Conjecture
:Conjecture rdf:type owl:Class ;
            rdfs:subClassOf <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#ToulminArgument> .


###  http://coolfutures.net/rdf/Design_Conjectures#DesignFeature
:DesignFeature rdf:type owl:Class ;
               rdfs:comment "DesignFeature is what Sandoval calls Embodiment."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#DiscoursePractice
:DiscoursePractice rdf:type owl:Class ;
                   rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#InstructionalChallenge
:InstructionalChallenge rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#LearningDesign
:LearningDesign rdf:type owl:Class ;
                rdfs:comment "A learnining design about which we are going to make conjectures. It is addressing an instructional challenge"@en .


###  http://coolfutures.net/rdf/Design_Conjectures#LearningOutcome
:LearningOutcome rdf:type owl:Class ;
                 rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Material
:Material rdf:type owl:Class ;
          rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#Measure
:Measure rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#MediatingProcess
:MediatingProcess rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#MotivationOutcome
:MotivationOutcome rdf:type owl:Class ;
                   rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Observable
:Observable rdf:type owl:Class ;
            rdfs:subClassOf :MediatingProcess .


###  http://coolfutures.net/rdf/Design_Conjectures#ObservableBehavior
:ObservableBehavior rdf:type owl:Class ;
                    rdfs:subClassOf :Observable .


###  http://coolfutures.net/rdf/Design_Conjectures#ObservableInteraction
:ObservableInteraction rdf:type owl:Class ;
                       rdfs:subClassOf :Observable .


###  http://coolfutures.net/rdf/Design_Conjectures#Observation
:Observation rdf:type owl:Class ;
             rdfs:subClassOf :Measure .


###  http://coolfutures.net/rdf/Design_Conjectures#Outcome
:Outcome rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#ParticipantArtifact
:ParticipantArtifact rdf:type owl:Class ;
                     rdfs:subClassOf :MediatingProcess .


###  http://coolfutures.net/rdf/Design_Conjectures#RolesAndResponsibilities
:RolesAndResponsibilities rdf:type owl:Class ;
                          rdfs:subClassOf :DesignFeature ;
                          rdfs:comment "How participants, such as students and teachers, are expted to participate in tasks, the roles and responsibilities they take on. Different from pre-requisites, which are modelled as Qualifiers in the Conjectures."^^xsd:string ;
                          rdfs:label "Roles and Responsibilities"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#Scale
:Scale rdf:type owl:Class ;
       rdfs:subClassOf :Measure .


###  http://coolfutures.net/rdf/Design_Conjectures#SocialOutcome
:SocialOutcome rdf:type owl:Class ;
               rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#TaskStructure
:TaskStructure rdf:type owl:Class ;
               rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#Tool
:Tool rdf:type owl:Class ;
      rdfs:subClassOf :DesignFeature .


#################################################################
#    Individuals
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#_Backing02
:_Backing02 rdf:type owl:NamedIndividual ,
                     <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#Backing> ;
            <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "Teacher observations on similar designs"^^xsd:string ;
            rdfs:comment "Backing from experience of teacher with similar designs"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_Claim01
:_Claim01 rdf:type owl:NamedIndividual ,
                   <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#Claim> ;
          <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "I am the text of _Claim01"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_Claim02
:_Claim02 rdf:type owl:NamedIndividual ,
                   <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#Claim> ;
          <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "I am the text of _Claim02"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_Claim03
:_Claim03 rdf:type owl:NamedIndividual ;
          <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "Keeping  a reflection journal will increase the number of reflections"^^xsd:string ;
          rdfs:comment "some design claim"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_Claim04
:_Claim04 rdf:type owl:NamedIndividual ;
          <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "I am the text for _Claim04"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_DesignConjecture01
:_DesignConjecture01 rdf:type owl:NamedIndividual ,
                              :Conjecture ;
                     <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasClaim> :_Claim03 ;
                     <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasData> :_MediatingProcessReflectionJournal ;
                     <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasQualifier> <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#_TQualifier01> ;
                     <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "The gist of the conjecture"^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_DesignFeatureCollaborativeInquiry
:_DesignFeatureCollaborativeInquiry rdf:type owl:NamedIndividual ,
                                             :DiscoursePractice ;
                                    :hasConjecture :_DesignConjecture01 ;
                                    <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "This particular discourse practice: ..."^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_InstructionalChallenge01
:_InstructionalChallenge01 rdf:type owl:NamedIndividual ,
                                    :InstructionalChallenge ;
                           <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "Scientific argumentation requires appropriation of discursive practices."^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_LearningConjecture01
:_LearningConjecture01 rdf:type owl:NamedIndividual ,
                                :Conjecture ;
                       <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasClaim> :_Claim04 ;
                       <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasData> :_Outcome01 .


###  http://coolfutures.net/rdf/Design_Conjectures#_LearningDesign01
:_LearningDesign01 rdf:type owl:NamedIndividual ,
                            :LearningDesign ;
                   :hasConjecture :_PedagogyConjecture01 ;
                   :hasDesignFeature :_DesignFeatureCollaborativeInquiry ;
                   :respondsTo :_InstructionalChallenge01 ;
                   <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#text> "This design fosters scientific argumentation by collaboration and epistemic reflection. The *key new idea* is affording and scaffolding epistemic reflection."^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#_MeasurementScale01
:_MeasurementScale01 rdf:type owl:NamedIndividual ,
                              :Scale .


###  http://coolfutures.net/rdf/Design_Conjectures#_MediatingProcessReflectionJournal
:_MediatingProcessReflectionJournal rdf:type owl:NamedIndividual ;
                                    :hasConjecture :_LearningConjecture01 .


###  http://coolfutures.net/rdf/Design_Conjectures#_Outcome01
:_Outcome01 rdf:type owl:NamedIndividual .


###  http://coolfutures.net/rdf/Design_Conjectures#_PedagogyConjecture01
:_PedagogyConjecture01 rdf:type owl:NamedIndividual ;
                       <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasBacking> :_Backing02 ;
                       <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#hasClaim> :_Claim02 .


