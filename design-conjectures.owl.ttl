#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AttitudeOutcome
                :LearningOutcome
                :MotivationOutcome
                :SocialOutcome
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :DiscoursePractice
                :Material
                :RolesAndResponsibilities
                :TaskStructure
                :Tool
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :_Backing02
                        <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#_TBacking01>
                      )
] .


[ rdf:type owl:AllDifferent ;
