@prefix : <http://coolfutures.net/rdf/Design_Conjectures#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.coolfutures.net/rdf/Design_Conjectures/dcon#> .

<http://www.coolfutures.net/rdf/Design_Conjectures/dcon#> rdf:type owl:Ontology ;
                                                           owl:imports <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#> ;
                                                           <http://purl.org/dc/terms/creator> "Peter Reimann"^^xsd:string ;
                                                           rdfs:comment "Initial work on an ontology for design conjecture claims."@en ;
                                                           owl:versionInfo "version 1.0"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#hasCondition
:hasCondition rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Conjecture ;
              rdfs:comment "A conjecture has one or more conditions"@en ;
              rdfs:seeAlso "hasConsequence"@en .


###  http://coolfutures.net/rdf/Design_Conjectures#hasConsequence
:hasConsequence rdf:type owl:ObjectProperty ;
                rdfs:domain :Conjecture .


###  http://coolfutures.net/rdf/Design_Conjectures#hasEmbodiment
:hasEmbodiment rdf:type owl:ObjectProperty ;
               rdfs:domain :PedagogyConjecture ;
               rdfs:range :DesignFeature ;
               rdfs:comment "A PedagogicalConjecture has one or more DesignFeatures."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#hasPedagogy
:hasPedagogy rdf:type owl:ObjectProperty ;
             rdfs:domain :DesignFeature ,
                         :LearningDesign ;
             rdfs:range :PedagogyConjecture ;
             rdfs:comment "A LearningDesign and a DesignFeature should have a (high level) pedagogy conjecture."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#hasWarrant
:hasWarrant rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Conjecture ;
            rdfs:comment "A conjecture should have some warrant. The warrant  can be from data or from analogy. The range is some entity that functions as a data source (e.g, a study) or as an analogical case."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#hasWarrantFromData
:hasWarrantFromData rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasWarrant ;
                    owl:propertyDisjointWith :hasWarrentFromAnalogy .


###  http://coolfutures.net/rdf/Design_Conjectures#hasWarrentFromAnalogy
:hasWarrentFromAnalogy rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasWarrant .


###  http://coolfutures.net/rdf/Design_Conjectures#isMeasureFor
:isMeasureFor rdf:type owl:ObjectProperty ;
              owl:inverseOf :isMeasuredBy ;
              rdfs:domain :Measure ;
              rdfs:comment "A measure can be for some Outcome and for some MediatingProcess and for some LearningConjecture."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#isMeasuredBy
:isMeasuredBy rdf:type owl:ObjectProperty ;
              rdfs:range :Measure .


#################################################################
#    Classes
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#AttitudeOutcome
:AttitudeOutcome rdf:type owl:Class ;
                 rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Conjecture
:Conjecture rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#DesignConjecture
:DesignConjecture rdf:type owl:Class ;
                  rdfs:subClassOf :Conjecture ,
                                  <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#ToulminArgument> ;
                  rdfs:comment "A conjecture about how a design feature affects behavior and the production of some artefact(s)."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#DesignFeature
:DesignFeature rdf:type owl:Class ;
               rdfs:comment "DesignFeature is what Sandoval calls Embodiment."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#DiscoursePractice
:DiscoursePractice rdf:type owl:Class ;
                   rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#InstructionalChallenge
:InstructionalChallenge rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#LearningConjecture
:LearningConjecture rdf:type owl:Class ;
                    rdfs:subClassOf :Conjecture ,
                                    <http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#ToulminArgument> ;
                    rdfs:comment "A conjecture about how a mediating process affects learning."@en .


###  http://coolfutures.net/rdf/Design_Conjectures#LearningDesign
:LearningDesign rdf:type owl:Class ;
                rdfs:comment "A learnining design about which we are going to make conjectures. It is addressing an instructional challenge"@en .


###  http://coolfutures.net/rdf/Design_Conjectures#LearningOutcome
:LearningOutcome rdf:type owl:Class ;
                 rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Material
:Material rdf:type owl:Class ;
          rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#Measure
:Measure rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#MeasurementConjecture
:MeasurementConjecture rdf:type owl:Class ;
                       rdfs:subClassOf :Conjecture ;
                       rdfs:comment "A claim about how a certain procedure measures a certain construct."^^xsd:string .


###  http://coolfutures.net/rdf/Design_Conjectures#MediatingProcess
:MediatingProcess rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#MotivationOutcome
:MotivationOutcome rdf:type owl:Class ;
                   rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#Observable
:Observable rdf:type owl:Class ;
            rdfs:subClassOf :MediatingProcess .


###  http://coolfutures.net/rdf/Design_Conjectures#ObservableBehavior
:ObservableBehavior rdf:type owl:Class ;
                    rdfs:subClassOf :Observable .


###  http://coolfutures.net/rdf/Design_Conjectures#ObservableInteraction
:ObservableInteraction rdf:type owl:Class ;
                       rdfs:subClassOf :Observable .


###  http://coolfutures.net/rdf/Design_Conjectures#Observation
:Observation rdf:type owl:Class ;
             rdfs:subClassOf :Measure .


###  http://coolfutures.net/rdf/Design_Conjectures#Outcome
:Outcome rdf:type owl:Class .


###  http://coolfutures.net/rdf/Design_Conjectures#ParticipantArtifact
:ParticipantArtifact rdf:type owl:Class ;
                     rdfs:subClassOf :MediatingProcess .


###  http://coolfutures.net/rdf/Design_Conjectures#ParticipantStructure
:ParticipantStructure rdf:type owl:Class ;
                      rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#PedagogyConjecture
:PedagogyConjecture rdf:type owl:Class ;
                    rdfs:subClassOf :Conjecture ;
                    rdfs:comment "High level conjecture about how to support learning in some context. (What Sandoval calls a high level conjecture.)"@en .


###  http://coolfutures.net/rdf/Design_Conjectures#Scale
:Scale rdf:type owl:Class ;
       rdfs:subClassOf :Measure .


###  http://coolfutures.net/rdf/Design_Conjectures#SocialOutcome
:SocialOutcome rdf:type owl:Class ;
               rdfs:subClassOf :Outcome .


###  http://coolfutures.net/rdf/Design_Conjectures#TaskStructure
:TaskStructure rdf:type owl:Class ;
               rdfs:subClassOf :DesignFeature .


###  http://coolfutures.net/rdf/Design_Conjectures#Tool
:Tool rdf:type owl:Class ;
      rdfs:subClassOf :DesignFeature .


#################################################################
#    Individuals
#################################################################

###  http://coolfutures.net/rdf/Design_Conjectures#scale_demo
:scale_demo rdf:type owl:NamedIndividual ,
                     :Scale .


#################################################################
#    Annotations
#################################################################

<http://www.coolfutures.net/rdf/Design_Conjectures/toulminarg#creator> rdfs:comment "One or more people who have created or contributed."^^xsd:string .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AttitudeOutcome
                :LearningOutcome
                :MotivationOutcome
                :SocialOutcome
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :DesignConjecture
                :LearningConjecture
                :MeasurementConjecture
                :PedagogyConjecture
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :DiscoursePractice
                :Material
                :ParticipantStructure
                :TaskStructure
                :Tool
              )
] .


