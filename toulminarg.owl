#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Backing
                :Claim
                :Data
                :Qualifier
                :Rebuttal
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :QualifierApplication
                :RebuttalApplication
                :Warrant
              )
] .


[ rdf:type owl:AllDifferent ;
